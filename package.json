{
  "name": "my-web-app",
  "version": "1.0.0",
  "description": "A simple web app using React for frontend and Node.js for backend following DDD best practices.",
  "main": "backend/src/server.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "nodemon backend/src/server.js",
    "start:frontend": "npm start --prefix frontend",
    "build:frontend": "npm run build --prefix frontend",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^1.3.4",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "gh-pages": "^6.1.1",
    "jest": "^29.0.1",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "nasrosoft",
  "license": "MIT"
}
